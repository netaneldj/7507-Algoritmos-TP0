Object subclass: #Biblioteca	instanceVariableNames: 'estantes'	classVariableNames: ''	poolDictionaries: ''	category: '99093'!!Biblioteca methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:45'!agregarEstante: mi_estante	estantes add: mi_estante.! !!Biblioteca methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 16:37'!cantidadDeLibrosDe: nombre_autor	|cantidad_autor|	cantidad_autor := 0.	estantes do:[:mi_estante | cantidad_autor := ((mi_estante cantidadAutor: nombre_autor) + cantidad_autor)].	^ cantidad_autor.! !!Biblioteca methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 19:02'!cantidadDeLibrosEn: mi_estante	|cantidad_libros|	cantidad_libros := 0.	estantes do:[:el_estante |((el_estante obtenerNombre) = mi_estante )ifTrue:[cantidad_libros :=  el_estante cantidad ] ].	^ cantidad_libros.! !!Biblioteca methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 15:57'!initialize	estantes := OrderedCollection new.! !!Biblioteca methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 19:23'!ubicacionDelLibro: nombre_libro	|ubicacion|	ubicacion :=''.	estantes do:[:mi_estante | (mi_estante estaElLibro: nombre_libro) ifTrue: [ubicacion := mi_estante obtenerNombre ]].		^ ubicacion.! !Object subclass: #Estante	instanceVariableNames: 'libros nombre'	classVariableNames: ''	poolDictionaries: ''	category: '99093'!!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:59'!cantidadAutor: nombre_autor	|libros_autor|	libros_autor := libros select:[:mi_libro| mi_libro obtenerAutor = nombre_autor].	^ libros_autor size.		! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 19:02'!agregarLibro: nombre_libro autor: nombre_autor	|mi_libro|	mi_libro := Libro new.	mi_libro agregarTitulo: nombre_libro.	mi_libro agregarAutor: nombre_autor.	libros add: mi_libro.! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:50'!obtenerNombre	^ nombre.! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 18:37'!cantidad	^ libros size.! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 19:22'!estaElLibro: nombre_libro	|filtrada| 	filtrada := OrderedCollection new.	libros do:[:mi_libro |(mi_libro obtenerTitulo = nombre_libro)ifTrue: [ filtrada add: mi_libro ] ].	^ (filtrada notEmpty).! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 18:14'!initialize	libros := OrderedCollection new.	nombre := ''.! !!Estante methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:20'!establecerNombre: nombre_estante	nombre := nombre_estante.! !Object subclass: #Libro	instanceVariableNames: 'titulo autor'	classVariableNames: ''	poolDictionaries: ''	category: '99093'!!Libro methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:16'!agregarTitulo: nombre_libro	titulo := nombre_libro.! !!Libro methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:40'!obtenerTitulo	^titulo.! !!Libro methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:38'!obtenerAutor	^ autor.! !!Libro methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:17'!agregarAutor: nombre_autor	autor := nombre_autor.	! !!Libro methodsFor: 'all' stamp: 'NetanelJamilis 3/17/2017 14:31'!initialize	titulo := ''.	autor := ''.	! !