" 7507/9502 | 2017 1C | TP0 | Enunciado 1 "| biblioteca unEstante cantidadDeLibros otroEstante error ubicacion |error := false.Transcript clear.biblioteca := Biblioteca new.unEstante := Estante new.unEstante establecerNombre: 'Estante 1'.unEstante agregarLibro: 'Estudio en escarlata' autor: 'Doyle'.unEstante agregarLibro: 'Pharo By Example' autor: 'Ducasse'.unEstante agregarLibro: 'Operacion Masacre' autor: 'Walsh'.unEstante agregarLibro: 'Rayuela' autor: 'Cortazar'.biblioteca agregarEstante: unEstante.otroEstante := Estante new.otroEstante establecerNombre: 'Estante 2'.otroEstante agregarLibro: 'La isla del tesoro' autor: 'Stevenson'.otroEstante agregarLibro: 'Todos los fuegos el fuego' autor: 'Cortazar'.otroEstante agregarLibro: 'La Patagonia rebelde' autor: 'Bayer'.biblioteca agregarEstante: otroEstante.cantidadDeLibros := biblioteca cantidadDeLibrosEn: 'Estante 1'.(cantidadDeLibros = 4) ifFalse: [	Transcript show: 'Error: la cantidad de libros en el estante 1 debe ser 4.';cr.	error := true.].cantidadDeLibros := biblioteca cantidadDeLibrosDe: 'Cortazar'.(cantidadDeLibros = 2) ifFalse: [	Transcript show: 'Error: la cantidad de libros de Cortazar debe ser 2.';cr.	error := true.].cantidadDeLibros := biblioteca cantidadDeLibrosDe: 'Vargas Llosa'.(cantidadDeLibros = 0) ifFalse: [	Transcript show: 'Error: la cantidad de libros de Vargas Llosa debe ser 0.';cr.	error := true.].ubicacion := biblioteca ubicacionDelLibro: 'La isla del tesoro'.(ubicacion = 'Estante 2') ifFalse: [	Transcript show: 'Error: La isla del tesoro debe estar en el segundo estante.';cr.	error := true.].error ifTrue: [  	Transcript show: 'Resultado final: ERROR.'.]ifFalse: [	Transcript show: 'Resultado final: OK.'.].